package test10;

public class Main {

    public static void main(String[] args) {
        /*
         * インクリメントを使った計算A
         */
        int a = 5;
        System.out.println(++a);
        
        /*
         * インクリメントを使った計算B
         */
        int b = 5;
        
        System.out.println(b++);
        
        /*
         * デクリメントを使った計算c
         */
        int c = 5;
        System.out.println(--c);
        /*
         * デクリメントを使った計算D
         */
        int d = 5;
        System.out.println(d--);
    }

}
